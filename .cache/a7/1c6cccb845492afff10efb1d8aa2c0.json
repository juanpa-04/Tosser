etDotPositions(number) {\n    const diceFace = {\n        1:['d'],\n        2:['a','g'],\n        3:['a','d','g'],\n        4:['a','b','f','g'],\n        5:['a','b','d','f','g'],\n        6:['a','b','c','e','f','g'],\n    }\n    \n    return diceFace[number]\n}\n\nfunction createDice() {\n\n    const newDice  = document.createElement('div');\n    newDice.className = \"dice\"\n    return newDice;\n  \n}\n\nfunction changeFace(dice,number,type) {\n    clearParent(dice);\n    type(number,dice);\n}\n\nfunction createDotFace(number,parent) {\n    const positions = getDotPositions(number);\n\n    positions.forEach((pos) => {\n        let div = document.createElement('div');\n        div.className = \"dot\";\n        div.style.gridArea = pos;\n        appendToParent(parent,div)\n    });\n\n}\n\n"},"lineCount":null}},"error":null,"hash":"8dc89389a832e4e087d904bf1c386dae","cacheData":{"env":{}}}